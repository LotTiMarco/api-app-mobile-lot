    "/api.auth/login": {
      "post": {
        "tags": [
          "Seguridad"
        ],
        "summary": "Endpoint para login",
        "description": "Endpoint para obtener id de usuario, rol y token de acceso.",
        "operationId": "api.auth/login",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "example": {
              "status": "success",
              "data": {
                "userId": "...",
                "userRole": "...",
                "accessToken": "..."
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/LoginResponse"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/responses/LoginResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/UnauthorizedError"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/responses/UnauthorizedError"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/InternalServerError"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/responses/InternalServerError"
                }
              }
            }
          }
        },
        "security": [
          {
            "basicAuth": []
          }
        ],
        "deprecated": false
      }
